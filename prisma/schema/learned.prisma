model Learned {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    id_user   String
    id_card   String   @db.ObjectId
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    card      Card     @relation(fields: [id_card], references: [id])

    @@unique([id_user, id_card])
}
